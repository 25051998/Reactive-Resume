{
  "name": "reactive-resume",
  "version": "3.0.0",
  "description": "A free and open-source resume builder.",
  "author": "Amruth Pillai <im.amruth@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AmruthPillai/Reactive-Resume"
  },
  "keywords": [
    "resume builder",
    "creative resumes",
    "open source"
  ],
  "scripts": {
    "dev": "concurrently \"npm:app:dev\" \"npm:server:dev\"",
    "build": "concurrently \"npm:app:build\" \"npm:server:build\"",
    "start": "concurrently \"npm:app:start\" \"npm:server:start\"",
    "lint": "concurrently \"npm:app:lint\" \"npm:server:lint\"",
    "app:dev": "cd app && next dev -p $APP_PORT",
    "app:build": "cd app && next build",
    "app:start": "cd app && next start",
    "app:lint": "cd app && eslint --fix . && next lint",
    "server:dev": "cd server && nest start --watch",
    "server:build": "cd server && rimraf dist && nest build",
    "server:lint": "cd server && eslint --fix ./src",
    "server:start": "cd server && node dist/main"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.5",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.5",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.5",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.0.1",
    "bcrypt": "^5.0.1",
    "classnames": "^2.3.1",
    "cookie-parser": "^1.4.5",
    "eventemitter3": "^4.0.7",
    "joi": "^17.4.1",
    "morgan": "^1.10.0",
    "next": "11.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-modal": "^3.14.3",
    "recoil": "^0.4.0",
    "recoil-persist": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.36"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.0",
    "@nestjs/schematics": "^8.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/concurrently": "^6.2.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.9",
    "@types/react": "17.0.15",
    "@types/react-modal": "^3.12.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "autoprefixer": "^10.3.1",
    "concurrently": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.7",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
