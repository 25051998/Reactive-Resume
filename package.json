{
  "name": "reactive-resume",
  "version": "3.0.0",
  "description": "A free and open-source resume builder.",
  "author": "Amruth Pillai <im.amruth@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AmruthPillai/Reactive-Resume"
  },
  "keywords": [
    "resume builder",
    "creative resumes",
    "open source"
  ],
  "scripts": {
    "dev": "concurrently \"npm:app:dev\" \"npm:server:dev\"",
    "build": "concurrently \"npm:app:build\" \"npm:server:build\"",
    "start": "concurrently \"npm:app:start\" \"npm:server:start\"",
    "lint": "concurrently \"npm:app:lint\" \"npm:server:lint\"",

    "app:dev": "cd app && next dev",
    "app:build": "cd app && next build",
    "app:start": "cd app && next start",
    "app:lint": "cd app && next lint",

    "server:dev": "cd server && nest start --watch",
    "server:build": "cd server && rimraf dist && nest build",
    "server:lint": "cd server && prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "server:start": "cd server && node dist/main"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.0.4",
    "@nestjs/typeorm": "^8.0.1",
    "bcrypt": "^5.0.1",
    "classnames": "^2.3.1",
    "cookie-parser": "^1.4.5",
    "eventemitter3": "^4.0.7",
    "express-session": "^1.17.2",
    "joi": "^17.4.1",
    "morgan": "^1.10.0",
    "next": "11.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "react-modal": "^3.14.3",
    "recoil": "^0.3.1",
    "recoil-persist": "^2.9.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@types/concurrently": "^6.2.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0",
    "@types/react": "17.0.14",
    "@types/react-modal": "^3.12.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "autoprefixer": "^10.3.1",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "postcss": "^8.2.13",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.4",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
